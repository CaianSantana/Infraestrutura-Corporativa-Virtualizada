acl clients {
    localhost;
    192.168.56.0/24;
};

options {
    directory "/var/cache/bind";
    allow-recursion { clients; };
    allow-query { clients; };
    allow-transfer { none; };
    version "Nao disponivel";

    forwarders {
        1.1.1.1;
        8.8.8.8;
    };
    
    dnssec-validation auto;
    listen-on-v6 { any; };
};
